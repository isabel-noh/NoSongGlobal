ssword,\n        }\n        this.$store.dispatch('logIn', payload)\n      },\n      closeModal(){\n        this.$emit('closeModal')\n      }\n    }\n}\n</script>\n\n<style>\n.loginModal {\n  width: 20%;\n  height: auto;\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  right: 0;\n  left: 0;\n  z-index: 100;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}