{"ast":null,"code":"import LoginModal from '@/components/LoginModal';\nexport default {\n  name: 'App',\n  components: {\n    LoginModal\n  },\n  data() {\n    return {\n      isLogin: false,\n      openModal: false\n    };\n  },\n  methods: {\n    isLoggedIn() {\n      this.isLogin = this.$store.getters.isLogin;\n    },\n    closeModal() {\n      this.openModal = false;\n    },\n    logOut() {\n      this.$store.dispatch('logOut');\n    }\n  },\n  mounted() {\n    this.isLoggedIn();\n  },\n  updated() {\n    this.isLoggedIn();\n  }\n};","map":{"version":3,"mappings":"AAiCA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IACA;EACA;AACA","names":["name","components","LoginModal","data","isLogin","openModal","methods","isLoggedIn","closeModal","logOut","mounted","updated"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <nav>\n      <div class=\"nav1\">\n        <router-link :to=\"{ name : 'home' }\"> <span id=\"logo\">npm</span>  <span id=\"logo_desc\">new perspective of movie</span></router-link>\n      </div>\n      <div class=\"nav2\">\n        <router-link :to=\"{ name : 'info' }\">Info</router-link> |\n        <!-- 검색 기능 -->\n        <i class=\"bi bi-search\"></i>\n        <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"16\" height=\"16\" fill=\"currentColor\" class=\"bi bi-emoji-heart-eyes\" viewBox=\"0 0 16 16\"><path d=\"M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z\"/><path d=\"M11.315 10.014a.5.5 0 0 1 .548.736A4.498 4.498 0 0 1 7.965 13a4.498 4.498 0 0 1-3.898-2.25.5.5 0 0 1 .548-.736h.005l.017.005.067.015.252.055c.215.046.515.108.857.169.693.124 1.522.242 2.152.242.63 0 1.46-.118 2.152-.242a26.58 26.58 0 0 0 1.109-.224l.067-.015.017-.004.005-.002zM4.756 4.566c.763-1.424 4.02-.12.952 3.434-4.496-1.596-2.35-4.298-.952-3.434zm6.488 0c1.398-.864 3.544 1.838-.952 3.434-3.067-3.554.19-4.858.952-3.434z\"/></svg>\n        <router-link :to=\"{ name : 'journal' }\">Journal</router-link> | \n        <!-- user정보에 따라 바뀜 -->\n        <button v-if=\"!isLogin\" @click=\"openModal = !openModal\">Login</button>\n        <div class=\"btn-group\" v-if=\"isLogin\">\n          <button type=\"button\" class=\"btn btn-white dropdown-toggle\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\n            Action\n          </button>\n          <ul class=\"dropdown-menu\">\n            <li><a class=\"dropdown-item\" @click=\"logOut\">sign out</a></li>\n            <li><router-link class=\"dropdown-item\" :to=\"{ name : 'myPage' }\" >MyPage</router-link></li>\n          </ul>\n        </div>\n      </div>\n    </nav>\n    <Transition>\n      <LoginModal v-if=\"openModal\" @closeModal=\"closeModal\"/>\n    </Transition>\n    <router-view/>\n  </div>\n</template>\n\n<script>\nimport LoginModal from '@/components/LoginModal'\nexport default {\n  name: 'App',\n  components: {\n    LoginModal\n  },\n  data(){\n    return {\n      isLogin : false,\n      openModal: false,\n    }\n  },\n  methods: {\n    isLoggedIn(){\n      this.isLogin = this.$store.getters.isLogin\n    },\n    closeModal(){\n      this.openModal = false\n    },\n    logOut(){\n      this.$store.dispatch('logOut')\n    }\n  },\n  mounted(){\n    this.isLoggedIn()\n  },\n  updated(){\n    this.isLoggedIn()          \n  }\n}\n</script>\n\n\n<style>\n.v-application {\n  font-family: 'Hannah', sans-serif !important;\n}\n@font-face {\n  font-family: \"Hannah\";\n  font-style: normal;\n  src: url('./fonts/BMHANNA_11yrs_ttf.ttf') format(\"tff\");\n  font-weight: 400;\n}\n\n#app {\n  font-family: \"Hannah\", Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: black;\n  padding: 0px 15%;\n  background-color: #fff;\n  /* background-color: #2c3e50; */\n}\n\nnav {\n  font-family: 'Hannah';\n  padding: 30px 0px;\n  display: flex;\n  justify-content: space-between;\n}\n\nnav a {\n  font-family: \"Hannah\";\n  font-weight: bold;\n  color: black;\n  text-decoration: none;\n}\n\nnav a.router-link-exact-active {\n  color: #202945;\n}\n\n#logo {\n  font-size: 2rem;\n  color: #202945;\n  text-decoration: none;\n}\n\na #logo_desc {\n  font-size: 1rem;\n  color: lightgray;\n  text-decoration: none;\n}\n\n.nav2{\n  margin: auto 0px;\n}\n/* input 기본 스타일 초기화 */\n/* input {\n    -webkit-appearance: none;\n       -moz-appearance: none;\n            appearance: none;\n} */\n\n/* input type number 에서 화살표 제거 */\n/* input[type='number']::-webkit-inner-spin-button,\ninput[type='number']::-webkit-outer-spin-button {\n    -webkit-appearance: none;\n       -moz-appearance: none;\n            appearance: none;\n} */\n/* Select box 스타일 초기화 */ \n/* select {\n    -webkit-appearance: none;\n       -moz-appearance: none;\n            appearance: none;\n} */\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}